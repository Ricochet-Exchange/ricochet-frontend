{
  "root": "build/",
  "clean_urls": false,
  "routes": {
    "/**": "index.html"
  },
  "headers": {
    "/manifest.json": {
      "Access-Control-Allow-Origin": "https://gnosis-safe.io",
      "Access-Control-Allow-Methods": "GET",
      "Access-Control-Allow-Headers": "X-Requested-With, content-type, Authorization"
    }
  }
}